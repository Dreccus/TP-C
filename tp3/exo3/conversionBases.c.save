#include <stdio.h>

int checkNULL(char* s){
	if(s == NULL){
		return -1;
		}
	else{
		return 1;
		}
}

int str_len(char* s){
	int i;
	if(checkNULL(s) != 1){
		return -1;
		}
	for(i=0; s[i]!='\0'; i++) {}
		return i;
}

int checkLowerC(char* s){
	int i;
